{
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "requireConfigFile": false, 
    "ecmaVersion": 2020,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    },
    "babelOptions": { "configFile": "./.babelrc" }
  },
  "extends": ["plugin:react/recommended", "plugin:prettier/recommended"],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "auto"
      }
    ],
    "react/prop-types": "off",
    "no-var": "error",
    "no-dupe-keys": "error"
  }
}
